<li>
	<div class="walktime"><%= walktime.time_from_integer %>
		<div id="schedule_form_<%= walktime.id %>">
			<%= render 'walktimes/schedule_form', walktime: walktime if signed_in? %>
		</div>
		<% if current_user?(walktime.dog.user) && edit_page %>
			<%= link_to "delete", walktime, method: :delete, data: { confirm: "You sure?" }, title: walktime.time %>
		<% end %>
	</div>
</li>